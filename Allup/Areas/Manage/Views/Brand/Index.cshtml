@model PageNatedList<Brand>

@{
    ViewData["Title"] = "Index";
    int count = (Model.PageIndex - 1) * 3;
}

<div class="row">
    <div class="col-lg-2">
        <h1 class="h3 mb-4 text-gray-800">Brands Page</h1>
    </div>
    <div class="col-lg-2">
        <a class="btn btn-primary mb-4" asp-action="create" asp-controller="brand" asp-area="manage">Create</a>
    </div>
</div>
<div class="row indexContainer">
    @await Html.PartialAsync("_BrandIndexPartial", Model)
</div>

